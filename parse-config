shebadir="$base/target/${SDECFG_TRG_SHEBA:-sheba}"
x="$shebadir/pkgs/$pkg"
if [ -d "$x/" ]; then
	var_append mnemosyne_overlay ' ' "$x"
fi
x="$shebadir/patches/$pkg.patch"
if [ -s "$x" ]; then
	var_append patchfiles ' ' "$x"
fi
unset shebadir x
